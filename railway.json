{
  "projectName": "Distribuidora Paucara",
  "buildCommand": "npm run build && php artisan storage:link",
  "startCommand": "php artisan serve --host=0.0.0.0 --port=${PORT:-8000}",
  "variables": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "PORT": "8080",
    "APP_PORT": "8080",
    "SESSION_DRIVER": "database",
    "SESSION_ENCRYPT": "false",
    "QUEUE_CONNECTION": "database",
    "CACHE_DRIVER": "database",
    "LOG_CHANNEL": "stack",
    "LOG_LEVEL": "notice"
  }
}