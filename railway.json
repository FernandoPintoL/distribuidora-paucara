{
  "projectName": "Distribuidora Paucara",
  "buildCommand": "composer install && php artisan migrate:fresh --seed",
  "startCommand": "php artisan serve --host=0.0.0.0 --port=8000",
  "variables": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "PORT": "8000",
    "APP_PORT": "8000",
    "SESSION_DRIVER": "file",
    "SESSION_ENCRYPT": "false",
    "QUEUE_CONNECTION": "sync",
    "CACHE_DRIVER": "file",
    "LOG_CHANNEL": "stack",
    "LOG_LEVEL": "notice"
  }
}
